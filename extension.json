{
	"name": "RedirectThoseCategories",
	"version": "0.1.2",
	"author": [
		"tesinormed"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:RedirectThoseCategories",
	"descriptionmsg": "redirectthosecategories-desc",
	"license-name": "CC0-1.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43.0",
